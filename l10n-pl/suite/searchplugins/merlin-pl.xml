<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Merlin</ShortName>
<Description>Wyszukiwanie w ofercie Merlin.pl</Description>
<InputEncoding>ISO-8859-2</InputEncoding>
<Image width="16" height="16">data:image/x-icon;base64,</Image>
 <SearchForm>http://merlin.pl</SearchForm>
 <Url method="GET"
      template="http://merlin.pl/search"
      type="text/html">
  <Param name="sourceid" value="Mozilla-search" />
  <Param name="fraza" value="{searchTerms}" />
  <Param name="skad" value="crhhxmkohb" />
 </Url>
</SearchPlugin>
